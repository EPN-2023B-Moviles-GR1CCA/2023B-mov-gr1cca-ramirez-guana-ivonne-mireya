<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3B673AB7"
    android:id="@+id/layout_listar_consulta"
    tools:context=".listar_Consulta">

    <ImageView

        android:id="@+id/imageView2"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:background="@drawable/registro"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_listado_consultas"
        app:layout_constraintVertical_bias="0.506">

    </ImageView>

    <TextView
        android:id="@+id/tv_Consultas_listar"
        android:layout_width="412dp"
        android:layout_height="86dp"
        android:background="#673AB7"
        android:padding="18dp"
        android:text="Listado Consultas"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ListView
        android:id="@+id/lv_listado_consultas"
        android:layout_width="305dp"
        android:layout_height="358dp"
        android:layout_marginBottom="224dp"
        android:foregroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_Consultas_listar"
        app:layout_constraintVertical_bias="0.954" />

    <Button
        android:id="@+id/btn_crear_consulta"
        android:layout_width="99dp"
        android:layout_height="57dp"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="88dp"
        android:text="Crear"
        android:textColorLink="@android:color/holo_orange_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_listado_consultas" />

</androidx.constraintlayout.widget.ConstraintLayout>